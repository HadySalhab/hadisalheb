{
	"name": "hadisalhebdev",
	"version": "1.0.0",
	"description": "Hadi Salheb Website",
	"main": "index.js",
	"scripts": {
		"start": "webpack serve",
		"dev:design": "webpack --watch",
		"dev:hugo": "cd hugo && hugo server --disableFastRender",
		"dev": "concurrently \"npm run dev:hugo\" \"npm run dev:design\"",
		"build:design": "webpack",
		"clone": "kss --clone ksstemplate",
		"sass": "node-sass --output-style compressed ksstemplate/kss-assets/kss.scss ksstemplate/kss-assets/kss.css",
		"build:hugo": "cd hugo && hugo",
		"build": "rm -rf public && npm run build:design && npm run build:hugo",
		"link-upstream": "git remote add upstream https://github.com/",
		"sync": "git fetch && git checkout master && git merge upstream/master && git push",
		"reset": "rm -rf ./node_modules ./package-lock.json && npm install && npm run dev",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "Hadi Salheb",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^14.14.31",
		"assets-webpack-plugin": "^3.9.7",
		"atomic-algolia": "^0.3.17",
		"autoprefixer": "^9.8.6",
		"clean-webpack-plugin": "^1.0.0",
		"concurrently": "^4.1.0",
		"css-loader": "^1.0.1",
		"dart-sass": "^1.25.0",
		"extract-loader": "^5.1.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^6.2.0",
		"highlight.js": "^10.6.0",
		"http-server": "^0.11.1",
		"kss": "^3.0.1",
		"kss-webpack-plugin": "^1.5.0",
		"michelangelo": "^0.8.0",
		"mini-css-extract-plugin": "^0.4.4",
		"node-sass": "^4.14.1",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"postcss-loader": "^4.2.0",
		"sass": "^1.26.3",
		"sass-loader": "^8.0.2",
		"style-loader": "^0.23.1",
		"ts-loader": "^5.3.0",
		"typescript": "^3.1.6",
		"uglifyjs-webpack-plugin": "^2.0.1",
		"webpack": "^4.46.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.2"
	},
	"dependencies": {
		"@primer/css": "^15.2.0"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
